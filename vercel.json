{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ], 
    "redirects": [
        {
            "source": "/",
            "destination": "./frontend/index.html",
            "status": 200
        }
    ]
}