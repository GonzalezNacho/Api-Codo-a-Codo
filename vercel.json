{
    "version": 2,
    "name": "Api-Codo-a-Codo",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/login",
            "dest": "/index.js"
        },
        {
            "src": "/comments(/.*)?",
            "dest": "/index.js"
        },
        {
            "src": "/keepalive",
            "dest": "/index.js"
        },
        {
            "src": "/movies(/.*)?",
            "dest": "/index.js"
        },
        {
            "src": "/directors(/.*)?",
            "dest": "/index.js"
        },
        {
            "src": "/users(/.*)?",
            "dest": "/index.js"
        },
        {
            "src": "/assets/(.*)",
            "dest": "/frontend/assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        }
    ]
}