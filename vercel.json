{
    "version": 2,
    "name": "Api-Codo-a-Codo",
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/frontend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": [
                "POST",
                "GET",
                "PUT",
                "DELETE",
                "PATCH"
            ]
        }
    ]
}